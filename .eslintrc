{
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["import", "prettier", "standard"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": 0,
    "object-curly-spacing": "off",
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2,
    "indent": ["error", 2]
  }
}
